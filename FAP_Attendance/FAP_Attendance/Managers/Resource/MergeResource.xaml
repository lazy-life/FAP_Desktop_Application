<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter="clr-namespace:FAP_Attendance.Managers.Converter">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Button.xaml"/>
        <ResourceDictionary Source="Font.xaml"/>
        <ResourceDictionary Source="ColorGeneric.xaml"/>
        <ResourceDictionary Source="Icon.xaml"/>
        <ResourceDictionary Source="TextBox.xaml"/>
        <ResourceDictionary Source="TextBlock.xaml"/>
        <ResourceDictionary Source="Image.xaml"/>
        <ResourceDictionary Source="Border.xaml"/>
        <ResourceDictionary Source="ScrollView.xaml"/>
        <ResourceDictionary Source="ListView.xaml"/>
        <ResourceDictionary Source="ComboBox.xaml"/>
        <ResourceDictionary Source="StoryBoard.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <converter:CheckDataEmpty x:Key="CheckDataEmpty"/>
    <converter:ConvertIntToString x:Key="ConvertIntToString"/>
    <converter:PasswordConverter x:Key="PasswordConverter"/>
    <converter:ConvertAttendanceStatus x:Key="ConvertAttendanceStatus"/>
    <converter:ConvertSlotTime x:Key="ConvertSlotTime"/>
    <converter:ConvertSlotToString x:Key="ConvertSlotToString"/>
    <converter:HalfValueConverter x:Key="HalfValueConverter"/>
</ResourceDictionary>